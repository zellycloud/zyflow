{
  "name": "@zyflow/parser",
  "version": "1.0.0",
  "description": "OpenSpec 1.0 compliant tasks.md parser for ZyFlow",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",

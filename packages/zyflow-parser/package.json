    "test:watch": "vitest"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "openspec",
    "parser",
    "tasks",
    "zyflow"
  ],
  "license": "MIT"
}

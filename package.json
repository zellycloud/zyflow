{
  "name": "zyflow",
  "version": "0.5.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "server": "tsx server/index.ts",
    "kill:ports": "lsof -ti:3100,3200 | xargs kill -9 || true",
    "dev:all": "npm run kill:ports && concurrently \"npm run server\" \"npm run dev\"",
    "build:mcp": "tsc -p tsconfig.mcp.json",
    "build:plugin": "npx tsx scripts/build-plugin.ts",
    "mcp": "node dist/mcp-server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"server/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop zyflow-server",
    "pm2:restart": "pm2 restart zyflow-server",
    "pm2:logs": "pm2 logs zyflow-server",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit"
  },

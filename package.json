{
  "name": "zyflow",
  "version": "0.5.0",
  "description": "MoAI SPEC 기반 개발 플로우 관리 도구 - Claude Code MCP 서버",
  "license": "MIT",
  "author": "ZyFlow Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/zellycloud/zyflow.git"
  },
  "homepage": "https://github.com/zellycloud/zyflow",
  "type": "module",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "node-pty",
      "ssh2",
      "esbuild",
      "@swc/core"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "server": "tsx watch server/index.ts",
    "kill:ports": "lsof -ti:3100,3101,3200,3201 | xargs kill -9 2>/dev/null || true",
    "kill:zyflow": "lsof -ti:3100,3101 | xargs kill -9 2>/dev/null || true",
    "kill:_flow": "lsof -ti:3200,3201 | xargs kill -9 2>/dev/null || true",
    "dev:all": "npm run kill:ports && concurrently --kill-others-on-fail -n zyflow-api,zyflow-web,_flow-api,_flow-web -c blue,cyan,magenta,yellow \"env-cmd -f .env.zyflow npm run server\" \"env-cmd -f .env.zyflow npm run dev\" \"env-cmd -f .env._flow npm run server\" \"env-cmd -f .env._flow npm run dev\"",
    "start": "npm run kill:ports && concurrently \"npm run server\" \"npm run dev\"",
    "dev:zyflow": "npm run kill:zyflow && env-cmd -f .env.zyflow concurrently --kill-others-on-fail \"npm run server\" \"npm run dev\"",
    "dev:_flow": "npm run kill:_flow && env-cmd -f .env._flow concurrently --kill-others-on-fail \"npm run server\" \"npm run dev\"",
    "start:zyflow": "env-cmd -f .env.zyflow npm run server",
    "start:_flow": "env-cmd -f .env._flow npm run server",
    "start:build": "npm run build && npm run server",
    "build:mcp": "tsc -p tsconfig.mcp.json",
    "build:plugin": "npx tsx scripts/build-plugin.ts",
    "mcp": "node dist/mcp-server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"server/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop zyflow-server",
    "pm2:restart": "pm2 restart zyflow-server",
    "pm2:logs": "pm2 logs zyflow-server",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.7",
    "@ai-sdk/react": "^3.0.14",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/adk": "^0.2.2",
    "@google/genai": "^1.35.0",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@octokit/rest": "^22.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.10",
    "@uiw/react-markdown-preview": "^5.1.5",
    "@uiw/react-md-editor": "^4.0.11",
    "@zyflow/parser": "file:packages/zyflow-parser",
    "@zyflow/remote-plugin": "file:packages/zyflow-remote-plugin",
    "ai": "^6.0.14",
    "better-sqlite3": "^12.4.6",
    "chokidar": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "express": "^4.21.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.554.0",
    "mermaid": "^10.9.5",
    "nanoid": "^5.1.6",
    "node-pty": "^1.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^4.0.3",
    "remark-gfm": "^4.0.0",
    "sonner": "^2.0.7",
    "ssh2": "^1.17.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "ws": "^8.18.3",
    "zod": "^3.25.76",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/dompurify": "^3.2.0",
    "@types/express": "^5.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ssh2": "^1.15.5",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.14",
    "concurrently": "^9.1.0",
    "drizzle-kit": "^0.18.1",
    "env-cmd": "^11.0.0",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "jsdom": "^27.2.0",
    "memfs": "^4.56.10",
    "playwright": "^1.57.0",
    "prettier": "^3.7.1",
    "supertest": "^7.1.4",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
}
